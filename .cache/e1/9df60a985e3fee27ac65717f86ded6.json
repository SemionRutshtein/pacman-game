1.5rem;\n}\n\n#score {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 200px;\n  height: 50px;\n  border: 5px darkcyan solid;\n  background: black;\n  margin-top: 20px;\n  color: white;\n  font-family: Arial, Helvetica, sans-serif;\n  font-size: 2rem;\n}\n\n#start-button {\n  margin-top: 20px;\n  background-color: darkcyan;\n}\n\n.hide {\n  display: none;\n}\n\n.square {\n  display: block;\n}\n\n.blank {\n  background: black;\n}\n\n.wall {\n  border: 1px solid darkcyan;\n}\n\n.dot {\n  background: white;\n  border: 8px solid black;\n  border-radius: 50%;\n}\n\n.pill {\n  background: white;\n  border: 5px solid black;\n  border-radius: 50%;\n}\n\n.ghost {\n  position: relative;\n  border-radius: 10px 10px 2px 2px;\n  border: none;\n}\n\n.ghost::before {\n  content: '';\n  display: block;\n  position: absolute;\n  width: 3px;\n  height: 3px;\n  background: black;\n  border-radius: 50%;\n  left: 3px;\n  top: 5px;\n  border: 2px solid white;\n}\n\n.ghost::after {\n  content: '';\n  display: block;\n  position: absolute;\n  width: 3px;\n  height: 3px;\n  background: black;\n  border-radius: 50%;\n  right: 3px;\n  top: 5px;\n  border: 2px solid white;\n}\n\n.scared {\n  background: blue !important;\n}\n\n.blinky {\n  background: red;\n}\n\n.pinky {\n  background: pink;\n}\n\n.inky {\n  background: cyan;\n}\n\n.clyde {\n  background: orange;\n}\n\n.pacman {\n  background: yellow;\n  border: none;\n  border-radius: 50%;\n  clip-path: polygon(\n    0 0,\n    20px 0,\n    20px -5px,\n    8px 10px,\n    20px 25px,\n    20px 20px,\n    0 20px\n  );\n  animation: chew 0.2s linear forwards infinite;\n}\n\n@keyframes chew {\n  50% {\n    clip-path: polygon(\n      0 0,\n      20px 0,\n      20px 10px,\n      8px 10px,\n      20px 10px,\n      20px 20px,\n      0 20px\n    );\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"ca96ac3bb5a0124f2d570646181f5bbe","cacheData":{"env":{}}}